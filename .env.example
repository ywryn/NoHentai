# ExHentai Configuration
# 获取方式：登录 ExHentai 后，在浏览器开发者工具中查看 Cookie
EXHENTAI_BASE_URL=https://exhentai.org/favorites.php
EXHENTAI_COOKIE_MEMBER_ID=your_member_id_here
EXHENTAI_COOKIE_PASS_HASH=your_pass_hash_here
EXHENTAI_COOKIE_IGNEOUS=your_igneous_here

# Retry Configuration
# 最大重试次数
MAX_RETRIES=5
# 重试延迟时间（秒）
RETRY_DELAY_SECONDS=2.0

# Backup Configuration
# 保留的备份文件数量
BACKUP_KEEP_COUNT=5

# Output Paths (relative to project root)
# 输出文件路径
OUTPUT_RELATIVE_PATH=web-static/public/data/galleries.json
# 备份文件目录
BACKUP_RELATIVE_DIR=web-static/public/data/backup_data
